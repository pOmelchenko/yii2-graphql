{
    "name":"tsingsun/yii2-graphql",
    "description":"facebook graphql server side for yii2 php framework",
    "keywords": ["yii2", "graphql"],
    "type":"yii2-extension",
    "license": "BSD-3-Clause",
    "authors":[
        {
            "name":"QingShan li",
            "email":"21997272@qq.com"
        }
    ],
    "minimum-stability":"dev",
    "require":{
        "php": ">=7.4.0",
        "yiisoft/yii2":"~2.0.10",
        "webonyx/graphql-php": "^0.13.0",
        "ecodev/graphql-upload": "^4.0.0",
        "laminas/laminas-diactoros": "^2.13"
    },
    "require-dev":{
        "phpunit/phpunit": "^7.5",
        "yiisoft/yii2": "~2.0.10",
        "yidas/yii2-composer-bower-skip": "^2.0"
    },
    "autoload":{
        "psr-4":{
            "yii\\graphql\\":"src"
        }
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "extra": {
        "yii\\composer\\bower-skip": true
    }
}
